{
  "containerDefinitions": [
    {
      "name": "app",
      "image": "636851749897.dkr.ecr.eu-north-1.amazonaws.com/ecs-platform-dev-api:d379dc8",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "PORT",
          "value": "8000"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "DB_SECRET",
          "valueFrom": "arn:aws:secretsmanager:eu-north-1:636851749897:secret:ecs-platform-dev/db-08999d7a-OubJqS"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ecs-platform-dev",
          "awslogs-region": "eu-north-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "family": "ecs-platform-dev-task",
  "executionRoleArn": "arn:aws:iam::636851749897:role/ecs-platform-dev-ecs-task-execution",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
